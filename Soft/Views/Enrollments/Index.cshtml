@using Contoso.Pages.HtmlHelpers
@using Contoso.Pages.Constants
@using System.Linq.Expressions;
@model IEnumerable<EnrollmentView>

@{
    Layout = Layouts.Index;
    ViewData[Datas.Title] = Texts.Enrollments;
    var columns = new List<Expression<Func<EnrollmentView, dynamic>>>{
        {x => x.CourseID },
        {x => x.CourseName },
        {x => x.StudentID },
        {x => x.StudentName},
        {x => x.Grade}
    };
    dynamic getId(EnrollmentView c) => c.ID;
}

@Html.ViewList(Model, columns, getId)

