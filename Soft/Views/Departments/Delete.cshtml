@model Department
@using Contoso.Pages.Constants
@using Contoso.Pages.HtmlHelpers

@{
	ViewData[Data.Title] = nameof(Department);
	ViewData[Data.ItemID] = Model.ID;
	Layout = Layouts.Delete;
}

<partial name="_ViewDepartments" model="@Model" />

<dt class="col-sm-2">
	@Html.DisplayNameFor(m => m.RowVersion)
</dt>
<dd class="col-sm-10">
	@Html.DisplayFor(m => m.RowVersion)
</dd>
<dt class="col-sm-2">
	@Html.DisplayNameFor(m => m.Administrator)
</dt>
<dd class="col-sm-10">
	@Html.DisplayFor(m => m.Administrator.FullName)
</dd>
