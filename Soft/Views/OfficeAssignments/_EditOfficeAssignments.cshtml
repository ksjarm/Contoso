@using Contoso.Pages.Constants;
@using Contoso.Pages.HtmlHelpers
@model OfficeAssignmentView

@{
    ViewData[Datas.Title] = nameof(OfficeAssignment);
    ViewData[Datas.ItemID] = Model?.ID;
}

@Html.SelectItem(x => x.InstructorID, "instructors")
@Html.EditItem(x => x.Location)
@Html.EditItem(x => x.Description)
@Html.EditItem(x => x.ValidFrom)
@Html.EditItem(x => x.ValidTo)